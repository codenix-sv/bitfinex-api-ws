    "bitfinex",
    "bitcoin",
    "btc",
    "ws",
    "web-socket",
    "bitfinex-api",
    "bitfinex-ws"
  ],
  "type": "library",
  "license": "BSD-3-Clause",
  "support": {
    "issues": "https://github.com/codenix-sv/bitfinex-api-ws/issues?state=open",
    "source": "https://github.com/codenix-sv/bitfinex-api-ws"
  },
  "authors": [
    {
      "name": "Vladymyr Svyryd",
      "email": "codenix.sv@gmail.com",
      "role": "Developer"
    }
  ],
  "require": {
